{
  "name": "cypress-framework",
  "version": "1.0.0",
  "description": "This is automation framework which is developed in Cypress JavaScript",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "reinstall": "npm run clean && npm install",
    "cypress": "cypress open",
    "cypress:debugger": "cross-env CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress open",
    "test": "cypress run",
    "head-test": "npm run test -- --headed",
    "chrome-test": "npm run test -- --browser chrome",
    "record-dashboard-test": "npm run test -- --record --key 9f9b3c93-05df-4d02-b8da-90186751bc43",
    "test-report": "npm run test -- --reporter cypress-mochawesome-reporter"
  },
  "author": "nhan.tran",
  "license": "ISC",
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/tests/BDD/step_definitions/**",
    "commonPath": "cypress/integration/tests/BDD/step_definitions/common/**",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "cypress": "^8.1.0",
    "cypress-cucumber-preprocessor": "^4.2.0",
    "cypress-iframe": "^1.0.1",
    "multiple-cucumber-html-reporter": "^1.18.0"
  }
}
